		{
			"name": "Test Signup",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = pm.response.json();",
							"pm.collectionVariables.set(\"userId\", jsonData.data.userId)",
							"",
							"pm.test(\"Status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"var firstName = pm.iterationData.get(\"userFirstName\");",
							"var lastName = pm.iterationData.get(\"userLastName\");",
							"var email = pm.iterationData.get(\"userEmail\");",
							"var password = pm.iterationData.get(\"defaultPassword\");",
							"var repeatPassword = pm.iterationData.get(\"repeatPassword\");",
							"",
							"pm.test(\"User's first name is not empty and is a string\", function () {",
							"    pm.expect(firstName).to.not.be.empty;",
							"    pm.expect(typeof firstName).to.eql(\"string\")",
							"});",
							"",
							"pm.test(\"User's last name is not empty and is a string\", function () {",
							"    pm.expect(lastName).to.not.be.empty;",
							"    pm.expect(typeof lastName).to.eql(\"string\")",
							"});",
							"",
							"pm.test(\"User's email is valid\", function () {   ",
							"    pm.expect(email).to.not.be.empty;",
							"    pm.expect(email).to.match(/^[a-zA-Z0-9]+([._-][a-zA-Z0-9]+)*@[a-z0-9-]+\\.[a-z]{2,}$/);",
							"});",
							"",
							"pm.test(\"The password is not empty and meets length requirement\", function () {",
							"    pm.expect(password).to.not.be.empty;",
							"    pm.expect(password).to.match(/^[a-zA-Z0-9]|[a-zA-Z\\W]$/);",
							"});",
							"",
							"pm.test(\"The repeatPassword matches the password\", function () {",
							"    pm.expect(repeatPassword).to.not.be.empty;",
							"    pm.expect(repeatPassword).to.eql(password);",
							"});",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "accept",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"name\": \"{{userFirstName}}\",\n    \"lastName\": \"{{userLastName}}\",\n    \"email\": \"{{userEmail}}\",\n    \"password\": \"{{defaultPassword}}\",\n    \"repeatPassword\": \"{{repeatPassword}}\"\n}",
